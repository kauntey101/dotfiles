# mysql --login-path alias
# this function is used to login into mysql servers which info is saved in config 
mysqlx() {
    local login_path="$1"
    shift
    mysql --login-path="$login_path" "$@"
}


mysqldumpx() {
    local login_path="$1"
    shift
    mysqldump --login-path="$login_path" "$@"
}


alias ls="colorls"
alias cls="clear"
